    <title>Orkoid</title>
    <baseDescription>NAME is Orkoid. As such, war is his only passion, with most basic menial tasks being performed only to fuel the warmachine.</baseDescription>
    <bodyTypeMale>Male</bodyTypeMale>
    <bodyTypeGlobal>Male</bodyTypeGlobal>
    <bodyTypeFemale>Male</bodyTypeFemale>
    <slot>Childhood</slot>
    <workDisables>
      <li>Intellectual</li>
      <li>Social</li>
      <li>Caring</li>
      <li>Cleaning</li>
      <li>Artistic</li>
    </workDisables>
    <skillGains>
    </skillGains>
    <spawnCategories>
      <li>Ork_Standard</li>
      <li>Ork_Nob</li>
      <li>Ork_Gretchin</li>
    </spawnCategories>
  </AlienRace.BackstoryDef>

