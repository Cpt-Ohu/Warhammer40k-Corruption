<Defs>
  <AlienRace.BackstoryDef>
    <defName>DE_Trueborn</defName>
    <title>Trueborn</title>
    <baseDescription>NAME was naturally conceived and born by a Dark Eldar couple, giving him an elevated status in society.</baseDescription>
    <bodytypeMale>Male</bodytypeMale>
    <bodyTypeFemale>Female</bodyTypeFemale>
    <slot>Childhood</slot>
    <workDisables>
      <li>PlantWork</li>
      <li>ManualDumb</li>
    </workDisables>
    <skillGains>
      <li>
        <defName>Melee</defName>
        <amount>3</amount>
      </li>
    </skillGains>
    <spawnCategories>
      <li>DarkEldar</li>
    </spawnCategories>
  </AlienRace.BackstoryDef>

